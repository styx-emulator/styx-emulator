{
  "image": "ghcr.io/styx-emulator/styx-emulator/styx-ci:main",
  "mounts": [
    "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
  ],
  "containerEnv": {
    "IN_CONTAINER": "1"
  },
  "name": "Styx Environment",
  "onCreateCommand": "cd ${containerWorkspaceFolder} && direnv allow . && just container-setup"
}
